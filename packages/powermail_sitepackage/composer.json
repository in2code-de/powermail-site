{
  "name": "in2code/powermail-sitepackage",
  "type": "typo3-cms-extension",
  "description": "Powermail development site",
  "homepage": "https://www.in2code.de/",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "require": {
    "typo3/cms-about": "*",
    "typo3/cms-adminpanel": "*",
    "typo3/cms-backend": "*",
    "typo3/cms-belog": "*",
    "typo3/cms-beuser": "*",
    "typo3/cms-extbase": "*",
    "typo3/cms-extensionmanager": "*",
    "typo3/cms-felogin": "*",
    "typo3/cms-filelist": "*",
    "typo3/cms-filemetadata": "*",
    "typo3/cms-fluid-styled-content": "*",
    "typo3/cms-fluid": "*",
    "typo3/cms-frontend": "*",
    "typo3/cms-impexp": "*",
    "typo3/cms-info": "*",
    "typo3/cms-install": "*",
    "typo3/cms-lowlevel": "*",
    "typo3/cms-recordlist": "*",
    "typo3/cms-redirects": "*",
    "typo3/cms-reports": "*",
    "typo3/cms-rte-ckeditor": "*",
    "typo3/cms-scheduler": "*",
    "typo3/cms-seo": "*",
    "typo3/cms-setup": "*",
    "typo3/cms-sys-note": "*",
    "typo3/cms-t3editor": "*",
    "typo3/cms-tstemplate": "*",
    "typo3/cms-viewpage": "*"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "powermail_sitepackage"
    }
  },
  "autoload": {
    "psr-4": {
      "In2code\\PowermailSitepackage\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "In2code\\PowermailSitepackage\\Tests\\": "Tests"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "scripts": {
    "setup": [
      "npm install"
    ],
    "post-install-cmd": [
      "@setup"
    ]
  }
}
